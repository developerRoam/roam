{
  "name": "roam",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "NODE_ENV=test YOURPACKAGE_COVERAGE=1 node ./node_modules/.bin/mocha specs/**/*_spec.js --require blanket --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "blanket": "^1.2.3",
    "body-parser": "~1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cookie-parser": "~1.4.3",
    "coveralls": "^3.0.0",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.15.5",
    "mocha": "~3.x.x",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "~1.9.0",
    "serve-favicon": "~2.4.5"
  },
  "config": {
    "blanket": {
      "pattern": [
        ""
      ],
      "data-cover-never": [
        "node_modules",
        "specs",
        "client"
      ]
    }
  }
}
